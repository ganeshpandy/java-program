class starborder2
{
public static void main(String[]args)
{
int n=5;
for(int i=0;i<n;i++)//responsible for no of rows
{
for(int j=0;j<n;j++)//responsible for no of column
{
	if(i==n/2||j==n/2)
System.out.print("*"+" ");
else
	System.out.print(" "+" ");
}
System.out.println();//move the print cursor to next line
}
}
}
