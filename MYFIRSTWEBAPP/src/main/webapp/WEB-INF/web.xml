<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MYFIRSTWEBAPP</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  
  	<servlet-name>DemoApp</servlet-name>
  	<servlet-class>com.jspider.myfirstwebapp.Demoapp</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>DemoApp</servlet-name>
  <url-pattern>/date</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>QueryString</servlet-name>
  <servlet-class>com.jspider.myfirstwebapp.QueryString</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>QueryString</servlet-name>
  <url-pattern>/string</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>CreateCookiesServlet</servlet-name>
  <servlet-class>com.jspiders.creatingcookies.CreateCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>CreateCookiesServlet</servlet-name>
  <url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ReadTheCookiesServlet</servlet-name>
  <servlet-class>com.jspiders.creatingcookies.ReadTheCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ReadTheCookiesServlet</servlet-name>
  <url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  
</web-app>